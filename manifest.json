{
    "manifest_version": 2,

    "name": "KSU Remaining",
    "description": "This extension notifies you when seats become available in classes you need to take",
    "version": "1.024",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "ksu.html"

    },
    "background": {
        "scripts": ["url.js"]
    },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],


    "permissions":[
        "tabs"
    ]
}
